#!/usr/bin/make
# Makefile generated by XPJ for OSX

DEPSDIR = .deps
APPLE_DEV_ROOT := $(shell xcode-select -print-path)
APPLE_OSX_SDK_PATH := $(APPLE_DEV_ROOT)/Platforms/MacOSX.platform/Developer/SDKs
APPLE_OSX_SDK_CURRENT_VERSION := $(APPLE_OSX_SDK_PATH)/$(firstword $(shell ls $(APPLE_OSX_SDK_PATH)))
#default defines
OBJS_DIR  = build
RMDIR     = rm -fr
ECHO      = echo
ifneq ("$(wildcard $(APPLE_DEV_ROOT)/Toolchains)", "")
	CCLD      = $(APPLE_DEV_ROOT)/Toolchains/XcodeDefault.xctoolchain/usr/bin/clang++
	CXX       = $(APPLE_DEV_ROOT)/Toolchains/XcodeDefault.xctoolchain/usr/bin/clang -x c++
	CC        = $(APPLE_DEV_ROOT)/Toolchains/XcodeDefault.xctoolchain/usr/bin/clang -x c
	RANLIB    = $(APPLE_DEV_ROOT)/Toolchains/XcodeDefault.xctoolchain/usr/bin/ranlib
	AR		 = $(APPLE_DEV_ROOT)/Toolchains/XcodeDefault.xctoolchain/usr/bin/ar
	STRIP     = $(APPLE_DEV_ROOT)/Toolchains/XcodeDefault.xctoolchain/usr/bin/strip
	OBJDUMP   = 
	OBJCOPY   = 
else
  CCLD      = $(APPLE_DEV_ROOT)/usr/bin/clang
  CXX       = $(APPLE_DEV_ROOT)/usr/bin/clang -x c++
  CC        = $(APPLE_DEV_ROOT)/usr/bin/clang -x c
  RANLIB    = $(APPLE_DEV_ROOT)/usr/bin/ranlib
  AR		 = $(APPLE_DEV_ROOT)/usr/bin/ar
  STRIP     = $(APPLE_DEV_ROOT)/usr/bin/strip
  OBJDUMP   = 
  OBJCOPY   = 
endif
-include Makedefs.OSX.mk

all: checked profile release debug

checked: build_PhysXCharacterKinematic_checked build_PhysXVehicle_checked build_PhysXExtensions_checked

profile: build_PhysXCharacterKinematic_profile build_PhysXVehicle_profile build_PhysXExtensions_profile

release: build_PhysXCharacterKinematic_release build_PhysXVehicle_release build_PhysXExtensions_release

debug: build_PhysXCharacterKinematic_debug build_PhysXVehicle_debug build_PhysXExtensions_debug

clean: clean_PhysXCharacterKinematic_checked clean_PhysXCharacterKinematic_profile clean_PhysXCharacterKinematic_release clean_PhysXCharacterKinematic_debug clean_PhysXVehicle_checked clean_PhysXVehicle_profile clean_PhysXVehicle_release clean_PhysXVehicle_debug clean_PhysXExtensions_checked clean_PhysXExtensions_profile clean_PhysXExtensions_release clean_PhysXExtensions_debug

include Makefile.PhysXCharacterKinematic.mk
include Makefile.PhysXVehicle.mk
include Makefile.PhysXExtensions.mk


